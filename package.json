{
  "name": "test-boilerplate",
  "version": "0.0.1",
  "description": "",
  "author": "clark",
  "license": "MIT",
  "scripts": {
    "createEntities": "rimraf entities & typeorm-model-generator -h 127.0.0.1 -d dbnest -p 3306 -u root -x root -e mysql -o ./src/entities --noConfig true --ce pascal --cp camel -a",
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "concurrently --handle-input \"wait-on dist/main.js && nodemon --delay 1500ms\" \"tsc -w -p tsconfig.build.json\"",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@nestjs/common": "^6.11.11",
    "@nestjs/core": "^6.11.11",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^6.11.11",
    "@nestjs/platform-socket.io": "^8.2.5",
    "@nestjs/swagger": "^3.1.0",
    "@nestjs/typeorm": "^6.3.4",
    "@nestjs/websockets": "^8.2.5",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.9.1",
    "dotenv": "^8.2.0",
    "ffmpeg": "0.0.4",
    "fluent-ffmpeg": "^2.1.2",
    "iconv-lite": "^0.6.3",
    "jimp": "^0.16.1",
    "mockjs": "^1.1.0",
    "mysql": "^2.18.1",
    "node-xlsx": "^0.17.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "qrcode-reader": "^1.0.4",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.7.1",
    "rxjs": "^6.5.5",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.24"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.11.11",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.6",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.17.19",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.7",
    "concurrently": "^4.1.2",
    "jest": "^23.6.0",
    "moment": "^2.29.1",
    "nestjs-redis": "^1.3.3",
    "nodemon": "^1.19.4",
    "prettier": "^1.19.1",
    "shelljs": "^0.8.4",
    "supertest": "^3.4.1",
    "ts-jest": "24.0.2",
    "ts-node": "8.1.0",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.16.0",
    "typescript": "3.4.3",
    "wait-on": "^3.3.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
